package com.telenav.data.datatypes.weather;

import java.util.Vector;

import com.telenav.data.datatypes.address.Stop;

/**
 * @author chrbu
 * @date 2011-12-31
 */
public class Weather
{
    private WeatherInfo currentWeatherInfo;

    private Vector weekWeatherInfos;

    private boolean isCanadianCarrier;

    private String locale;

    private Stop stop;

    public WeatherInfo getCurrentWeatherInfo()
    {
        return currentWeatherInfo;
    }

    public void setCurrentWeatherInfo(WeatherInfo currentWeatherInfo)
    {
        this.currentWeatherInfo = currentWeatherInfo;
    }

    public Vector getWeekWeatherInfos()
    {
        return weekWeatherInfos;
    }

    public void setWeekWeatherInfos(Vector weekWeatherInfos)
    {
        this.weekWeatherInfos = weekWeatherInfos;
    }

    public boolean isCanadianCarrier()
    {
        return isCanadianCarrier;
    }

    public void setCanadianCarrier(boolean isCanadianCarrier)
    {
        this.isCanadianCarrier = isCanadianCarrier;
    }

    public String getLocale()
    {
        return locale;
    }

    public void setLocale(String locale)
    {
        this.locale = locale;
    }

    public Stop getStop()
    {
        return stop;
    }

    public void setStop(Stop stop)
    {
        this.stop = stop;
    }

}
