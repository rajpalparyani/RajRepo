package com.telenav.map.opengl.java.proxy;

import com.telenav.map.opengl.java.TnMapEngine.Pickable;

/**
 *@author fqming (fqming@telenav.cn)
 *@date 2010-9-29
 */
public class TnMapPickableTrafficIncident extends Pickable
{
    public int m_severity;

    public boolean m_laneClosed;

    public int m_incidentType;

    public String m_crossStreet;

    public String m_message;

    public TnMapPickableTrafficIncident(int severity, boolean laneClosed, int incidentType, String crossStreet, String message)
    {
        m_severity = severity;
        m_laneClosed = laneClosed;
        m_incidentType = incidentType;
        m_crossStreet = crossStreet;
        m_message = message;
    }

    public int getSeverity()
    {
        return m_severity;
    }

    public boolean isLaneClosed()
    {
        return m_laneClosed;
    }

    public int getIncidentType()
    {
        return m_incidentType;
    }

    public String getCrossStreet()
    {
        return m_crossStreet;
    }

    public String getMessage()
    {
        return m_message;
    }
}
