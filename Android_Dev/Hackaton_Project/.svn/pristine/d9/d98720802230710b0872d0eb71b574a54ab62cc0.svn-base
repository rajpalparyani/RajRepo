<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="add_place_label">
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">#ff333333</item>
        <item name="android:textStyle">bold</item>
        <item name="android:gravity">center_vertical</item>
    </style>

    <style name="add_place_custom_button">
        <item name="android:textSize">19sp</item>
        <item name="android:textColor">#ff241f21</item>
        <item name="android:textStyle">bold</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">50dp</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_marginLeft">24dp</item>
        <item name="android:layout_marginRight">24dp</item>
    </style>

    <style name="add_place_custom_textfield">
        <item name="android:textSize">16sp</item>
        <item name="android:textStyle">normal</item>
        <item name="tnBackground">ftue_text_view_unfocused.9.png</item>
        <item name="android:singleLine">true</item>
        <item name="android:textColor">#ff1B1F22</item>
        <item name="android:textColorHint">#999999</item>
    </style>

    <style name="add_place_custom_address_line">
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">#ff858B92</item>
    </style>

    <!-- color -->
    <color name="addplaceListViewBackground">#ffffffff</color>
    <color name="addplaceCustomAddressFieldBackground">#fff8f7f7</color>
    <color name="addplaceCategoryListBackground">#ffeaeaea</color>
    <color name="addplaceCategoryItemText">#ff1b1f22</color>
    <color name="addplaceNewCategoryText">#ff505c67</color>
    <color name="addplaceCategoryListShadow">#ff999999</color>

    <!-- dimen -->
    <dimen name="addplaceCustomAddressPaddingLeft">6.5dp</dimen>

</resources>