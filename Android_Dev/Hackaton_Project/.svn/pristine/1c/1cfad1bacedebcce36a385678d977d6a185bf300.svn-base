<?xml version="1.0" encoding="utf-8"?>

<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:id="@+id/full_container" 
	android:layout_width="fill_parent"
	android:layout_height="wrap_content" 
	android:orientation="vertical"
	android:paddingLeft="8dip" 
	android:paddingRight="8dip" 
	android:paddingTop="@dimen/widget_port_paddingTop"
	android:paddingBottom="@dimen/widget_port_paddingBottom" 
	android:layout_marginLeft="@dimen/widget_port_marginLeft"
	android:layout_marginRight="@dimen/widget_port_marginRight"
	android:layout_marginTop="@dimen/full_port_marginTop"
	android:layout_marginBottom="@dimen/full_port_marginBottom"	
	android:background="@drawable/widgets_bg_unfocused">

	<LinearLayout
		android:layout_width="fill_parent"
		android:layout_height="wrap_content" 
		android:orientation="horizontal" 
		android:gravity="center_vertical"
		android:paddingRight="5dip"
		android:paddingBottom="3dip">
		<ImageView
			android:layout_width="wrap_content" 
			android:layout_height="wrap_content"
			android:layout_weight="0.05"
			android:src="@drawable/widgets_pin_icon_unfocused">
		</ImageView>
			
		<TextView 
			android:id="@+id/around"
	        android:layout_width="wrap_content"
			android:layout_height="wrap_content" 
	        android:paddingLeft="5dip"
	        android:paddingRight="5dip"
	        android:gravity="left|center_vertical"
	        android:singleLine="true"
	        android:ellipsize="end"
	        android:editable="false"
	        android:focusable="false"
	        android:inputType="none"
			android:textSize="14dip"
	        android:textStyle="normal"
			android:clickable="false" 
			android:textColor="@color/around"/>
			
		<TextView 
			android:id="@+id/Address"
	        android:layout_width="wrap_content"
			android:layout_height="fill_parent" 
			android:layout_weight="0.5"
	        android:paddingLeft="0dip"
	        android:paddingRight="10dip"
	        android:gravity="left|center_vertical"
	        android:singleLine="true"
	        android:ellipsize="end"
	        android:editable="false"
	        android:focusable="false"
	        android:inputType="none"
			android:textSize="14dip"
	        android:textStyle="bold"
			android:clickable="false" 
			android:textColor="@color/commonLabel"/>

		<ImageView
			android:layout_width="2dip" 
			android:layout_height="wrap_content"			
			android:layout_marginLeft="0dip" 
			android:layout_marginRight="5dip"
			android:layout_centerInParent="true" 
			android:src="@drawable/widgets_vline_unfocused">
		</ImageView>
					
		<ImageButton
			android:id="@+id/refresh"
			android:layout_width="wrap_content" 
			android:layout_height="wrap_content"	
			android:paddingLeft="0dip"
			android:paddingRight="0dip"
			android:paddingTop="2dip"
			android:paddingBottom="2dip"	
			android:background="@xml/widget_search_btn_bg"
			android:src="@xml/widget_refresh_btn_bg"
			android:clickable="true">
		</ImageButton>
	</LinearLayout>
	<ImageView
		android:layout_width="fill_parent" 
		android:layout_height="2dip"			
		android:layout_marginTop="@dimen/split_line_marginTop" 
		android:layout_marginBottom="@dimen/split_line_marginBottom" 
		android:background="@drawable/grey_split_line_unfocused">
	</ImageView>
	<LinearLayout
		android:id="@+id/onebox"
		android:layout_width="fill_parent"
		android:layout_height="fill_parent" 
		android:paddingTop="0dip"
		android:paddingBottom="0dip"
		android:orientation="horizontal" 		
		android:background="@xml/widget_onebox_bg"
		android:clickable="true" >
		
		<ImageView
			android:layout_width="wrap_content" 
			android:layout_height="wrap_content"
			android:layout_marginTop="8dip"
			android:src="@drawable/widgets_searchbox_search_icon_unfocused">
		</ImageView>
		
		<TextView 
			android:id="@+id/input"
	        android:layout_width="fill_parent"
			android:layout_height="fill_parent"
			android:paddingLeft="10dip" 	        
	        android:singleLine="true"
	        android:ellipsize="end"
	        android:editable="false"
	        android:focusable="true"
	        android:inputType="none"
			android:textSize="14dip"
	        android:textStyle="normal"
	        android:gravity="left|center_vertical"
	        android:textColor="@color/around"
	        android:textColorHint="#808080"
			android:clickable="false" >
		</TextView>		
	</LinearLayout>
	
	<LinearLayout
		android:layout_width="fill_parent"
		android:layout_height="wrap_content" 
		android:orientation="horizontal"
		android:paddingLeft="0dip" 
		android:paddingRight="0dip" 
		android:paddingTop="0dip"
		android:paddingBottom="0dip" 
		android:layout_marginLeft="0dip"
		android:layout_marginRight="0dip"
		android:layout_marginTop="5dip"
		android:layout_marginBottom="5dip">	
				
		<ImageButton
			android:id="@+id/coffee"
			android:layout_width="wrap_content" 
			android:layout_height="wrap_content"
			android:layout_weight="0.116"
			android:paddingLeft="2dip"
			android:paddingRight="2dip"
			android:paddingTop="2dip"
			android:paddingBottom="2dip"	
			android:background="@xml/widget_common_btn_bg"
			android:src="@xml/widget_coffee_btn_bg"
			android:clickable="true">
		</ImageButton>
		<ImageView
			android:id="@+id/coffeeline"
			android:layout_width="2dip" 
			android:layout_height="wrap_content"
			android:layout_weight="0.056"
			android:paddingLeft="0dip"
			android:paddingRight="0dip"
			android:paddingTop="3dip"
			android:paddingBottom="3dip">
		</ImageView>
		
		<ImageButton
			android:id="@+id/food"
			android:layout_width="wrap_content" 
			android:layout_height="wrap_content"
			android:layout_weight="0.116"
			android:paddingLeft="2dip"
			android:paddingRight="2dip"
			android:paddingTop="2dip"
			android:paddingBottom="2dip"	
			android:background="@xml/widget_common_btn_bg"
			android:clickable="true" 
			android:src="@xml/widget_food_btn_bg">
		</ImageButton>
		<ImageView
			android:id="@+id/foodline"
			android:layout_width="2dip" 
			android:layout_height="wrap_content"
			android:layout_weight="0.056"
			android:paddingLeft="0dip"
			android:paddingRight="0dip"
			android:paddingTop="3dip"
			android:paddingBottom="3dip">
		</ImageView>
		
		<ImageButton
			android:id="@+id/gas"
			android:layout_width="wrap_content" 
			android:layout_height="wrap_content"
			android:layout_weight="0.116"
			android:paddingLeft="2dip"
			android:paddingRight="2dip"
			android:paddingTop="2dip"
			android:paddingBottom="2dip"	
			android:background="@xml/widget_common_btn_bg"
			android:clickable="true" 
			android:src="@xml/widget_gas_btn_bg">
		</ImageButton>
		<ImageView
			android:id="@+id/gasline"
			android:layout_width="2dip" 
			android:layout_height="wrap_content"
			android:layout_weight="0.056"
			android:paddingLeft="0dip"
			android:paddingRight="0dip"
			android:paddingTop="3dip"
			android:paddingBottom="3dip">
		</ImageView>
		<ImageButton
			android:id="@+id/parking"
			android:layout_width="wrap_content" 
			android:layout_height="wrap_content"	
			android:layout_weight="0.116"
			android:paddingLeft="2dip"
			android:paddingRight="2dip"
			android:paddingTop="2dip"
			android:paddingBottom="2dip"	
			android:background="@xml/widget_common_btn_bg"
			android:clickable="true" 
			android:src="@xml/widget_parking_btn_bg">
		</ImageButton>
		<ImageView
			android:id="@+id/parkingline"
			android:layout_width="2dip" 
			android:layout_height="wrap_content"
			android:layout_weight="0.056"
			android:paddingLeft="0dip"
			android:paddingRight="0dip"
			android:paddingTop="3dip"
			android:paddingBottom="3dip">
		</ImageView>
		<ImageButton
			android:id="@+id/atm"
			android:layout_width="wrap_content" 
			android:layout_height="wrap_content"
			android:layout_weight="0.116"
			android:paddingLeft="2dip"
			android:paddingRight="2dip"
			android:paddingTop="2dip"
			android:paddingBottom="2dip"	
			android:background="@xml/widget_common_btn_bg"
			android:src="@xml/widget_atm_btn_bg"
			android:clickable="true">
		</ImageButton>
		
	</LinearLayout>
	
	<LinearLayout		
		android:layout_width="fill_parent"
		android:layout_height="wrap_content" 
		android:baselineAligned="false"
		android:orientation="horizontal">
		
		<LinearLayout
			android:id="@+id/mapContainer"
			android:layout_width="fill_parent"
			android:layout_height="fill_parent" 
			android:layout_weight="0.49"
			android:paddingLeft="1dip"
			android:paddingRight="1dip"
			android:paddingTop="1dip"
			android:paddingBottom="1dip"
			android:background="@drawable/widgets_map_bg_unfocused">
		</LinearLayout>

		<LinearLayout		
			android:layout_width="fill_parent"
			android:layout_height="wrap_content" 
			android:layout_weight="0.49"		
			android:layout_marginLeft="4dip"
			android:orientation="vertical">

			<LinearLayout		
				android:id="@+id/home_container"
				android:layout_width="fill_parent"
				android:layout_height="fill_parent" 
				android:layout_weight="0.50"		
				android:paddingLeft="0dip" 
				android:paddingRight="0dip" 
				android:paddingTop="0dip"
				android:paddingBottom="0dip" 
				android:orientation="vertical"
				android:gravity="center_vertical">
				<TextView	
					android:id="@+id/home"	
					android:layout_width="fill_parent"
					android:layout_height="fill_parent" 
			        android:paddingLeft="10dip"
					android:clickable="true"
					android:background="@xml/widget_home_btn_bg"
			        android:gravity="left|center_vertical"
			        android:singleLine="true"
			        android:inputType="none"
					android:textSize="16dip"
			        android:textStyle="bold"
					android:textColor="@color/widget_addr_text_color">
				</TextView>
			</LinearLayout>
			<ImageView
			    android:layout_width="fill_parent"
			    android:layout_height="2dip"
			    android:layout_marginBottom="1dip"
			    android:layout_marginTop="1dip"
			    android:background="@drawable/widgets_hline_unfocused" >
			</ImageView>
			<LinearLayout		
				android:id="@+id/work_container"
				android:layout_width="fill_parent"
				android:layout_height="fill_parent" 
				android:layout_weight="0.50"		
				android:paddingLeft="0dip" 
				android:paddingRight="0dip" 
				android:paddingTop="0dip"
				android:paddingBottom="0dip" 
				android:orientation="vertical"
				android:gravity="center_vertical">
				<TextView	
					android:id="@+id/work"	
					android:layout_width="fill_parent"
					android:layout_height="fill_parent" 
			        android:paddingLeft="10dip"
					android:clickable="true"
					android:background="@xml/widget_work_btn_bg"
			        android:gravity="left|center_vertical"
			        android:singleLine="true"
			        android:inputType="none"
					android:textSize="16dip"
			        android:textStyle="bold"
					android:textColor="@color/widget_addr_text_color">
				</TextView>
			</LinearLayout>
		</LinearLayout>
	</LinearLayout>
	
</LinearLayout>
