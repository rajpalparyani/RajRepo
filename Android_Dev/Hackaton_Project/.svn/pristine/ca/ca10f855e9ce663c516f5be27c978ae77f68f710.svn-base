package com.telenav.data.datatypes.poi;

/**
 *@author bduan
 *@date 2010-11-11
 */
public class Ad
{
    protected String adTag;
    protected String adLine;
    protected String adShortLine;
	protected String adSource;
    protected String adID;
    
    public Ad()
    {
        
    }
    
    public String getAdTag()
    {
        return adTag;
    }
    
    public void setAdTag(String adTag)
    {
        this.adTag = adTag;
    }
    
	public String getAdShortLine() 
	{
		return adShortLine;
	}

	public void setAdShortLine(String adShortLine) 
	{
		this.adShortLine = adShortLine;
	}

    public String getAdLine()
    {
        return adLine;
    }
    
    public void setAdLine(String adLine)
    {
        this.adLine = adLine;
    }

    public String getAdSource()
    {
        return adSource;
    }
    
    public void setAdSource(String adSource)
    {
        this.adSource = adSource;
    }

    public String getAdID()
    {
        return adID;
    }
    
    public void setAdID(String adID)
    {
        this.adID = adID;
    }
}
