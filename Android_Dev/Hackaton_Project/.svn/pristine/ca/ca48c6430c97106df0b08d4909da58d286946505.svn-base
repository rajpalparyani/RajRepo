package com.telenav.module.upsell;

/**
 * @author chrbu
 * @date 2012-12-18
 */
public class OfferTerm
{
    public static final int PERIOD_DAY_UNIT = 3; //Don't change value, coming from billing
    public static final int PERIOD_YEAR_UNIT = 0;//Don't change value, coming from billing
    
    private long offerTermId;

    private int payPeriod;

    private int payPeriodUnit;

    private String payType;

    private int promoPeriod;

    private int promoPeriodUnit;

    public long getOfferTermId()
    {
        return offerTermId;
    }

    public void setOfferTermId(long offerTermId)
    {
        this.offerTermId = offerTermId;
    }

    public int getPayPeriod()
    {
        return payPeriod;
    }

    public void setPayPeriod(int payPeriod)
    {
        this.payPeriod = payPeriod;
    }

    public int getPayPeriodUnit()
    {
        return payPeriodUnit;
    }

    public void setPayPeriodUnit(int payPeriodUnit)
    {
        this.payPeriodUnit = payPeriodUnit;
    }

    public String getPayType()
    {
        return payType;
    }

    public void setPayType(String payType)
    {
        this.payType = payType;
    }

    public int getPromoPeriod()
    {
        return promoPeriod;
    }

    public void setPromoPeriod(int promoPeriod)
    {
        this.promoPeriod = promoPeriod;
    }

    public int getPromoPeriodUnit()
    {
        return promoPeriodUnit;
    }

    public void setPromoPeriodUnit(int promoPeriodUnit)
    {
        this.promoPeriodUnit = promoPeriodUnit;
    }

}
