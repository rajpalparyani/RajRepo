package com.telenav.tnui.core;

/**
 * The command Event means that this will cause a module action directly.
 * <br />
 * For examples: Click a menu item, click the trackball, click the Dpad-Center, and click the enter key etc.
 * 
 *@author fqming (fqming@telenav.cn)
 *@date 2010-6-22
 */
public class TnCommandEvent
{
    private int commandId;
    
    public TnCommandEvent(int commandId)
    {
        this.commandId = commandId;
    }
    
    /**
     * Retrieve the command id.
     * 
     * @return commandId
     */
    public int getCommand()
    {
        return this.commandId;
    }
}
