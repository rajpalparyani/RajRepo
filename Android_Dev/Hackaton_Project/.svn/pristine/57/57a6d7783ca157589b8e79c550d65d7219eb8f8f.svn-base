package com.telenav.navservice;

public class NavServiceParameter
{
    private boolean isForeground;
    private boolean hasSetForeground;
    
    private String serverUrl;
    private boolean hasSetServerUrl;
    
    private String userId;
    private boolean hasSetUserId;
    
    private String carrierName;
    private boolean hasSetCarrierName;
    
    private String routeId;
    private boolean hasSetRouteId;
    
    private int pid;
    private boolean hasSetPid;
    
    private boolean logEnabled;
    private boolean hasSetLogEnabled;
    
    private String appVersion;
    private boolean hasSetAppVersion;
    
    private String appName;
    private boolean hasSetAppName;
    
    private boolean isForceStop;
    private boolean hasSetForceStop;
    
    private int pauseGpsTime; //used to pause the GPS for sometime. Unit is second.
    private boolean hasSetPauseGpsTime;
    
    public boolean isForeground()
    {
        return isForeground;
    }

    public void setForeground(boolean isForeground)
    {
        this.isForeground = isForeground;
        this.hasSetForeground = true;
    }

    public void setServerUrl(String url)
    {
        this.serverUrl = url;
        this.hasSetServerUrl = true;
    }
    
    public String getServerUrl()
    {
        return this.serverUrl;
    }
    
    public void setUserId(String userId)
    {
        this.userId = userId;
        this.hasSetUserId = true;
    }
    
    public String getUserId()
    {
        return this.userId;
    }
    
    public void setCarrierName(String carrierName)
    {
        this.carrierName = carrierName;
        this.hasSetCarrierName = true;
    }
    
    public String getCarrierName()
    {
        return this.carrierName;
    }
    
    public void setRouteId(String routeId)
    {
        this.routeId = routeId;
        this.hasSetRouteId = true;
    }
    
    public String getRouteId()
    {
        return this.routeId;
    }
    
    public boolean isLogEnabled()
    {
        return logEnabled;
    }

    public void setLogEnabled(boolean logEnabled)
    {
        this.logEnabled = logEnabled;
        this.hasSetLogEnabled = true;
    }

    public String getAppVersion()
    {
        return appVersion;
    }

    public void setAppVersion(String appVersion)
    {
        this.appVersion = appVersion;
        this.hasSetAppVersion = true;
    }

    public String getAppName()
    {
        return appName;
    }

    public void setAppName(String appName)
    {
        this.appName = appName;
        this.hasSetAppName = true;
    }

    public boolean isForceStop()
    {
        return isForceStop;
    }

    public void setForceStop(boolean forceStop)
    {
        this.isForceStop = forceStop;
        this.hasSetForceStop = true;
    }
    
    public int getPauseGpsTime()
    {
        return this.pauseGpsTime;
    }
    
    public void setPauseGpsTime(int time)
    {
        this.pauseGpsTime = time;
        this.hasSetPauseGpsTime = true;
    }
    
    public int getPid()
    {
        return pid;
    }
    
    public void setPid(int pid)
    {
        this.pid = pid;
        this.hasSetPid = true;
    }

    public boolean hasSetForeground()
    {
        return hasSetForeground;
    }

    public boolean hasSetServerUrl()
    {
        return this.hasSetServerUrl;
    }
    
    public boolean hasSetUserId()
    {
        return this.hasSetUserId;
    }
    
    public boolean hasSetCarrierName()
    {
        return this.hasSetCarrierName;
    }
    
    public boolean hasSetRouteId()
    {
        return this.hasSetRouteId;
    }
    
    public boolean hasSetLogEnabled()
    {
        return this.hasSetLogEnabled;
    }

    public boolean hasSetAppVersion()
    {
        return hasSetAppVersion;
    }

    public boolean hasSetAppName()
    {
        return hasSetAppName;
    }

    public boolean hasSetForceStop()
    {
        return hasSetForceStop;
    }
    
    public boolean hasSetPid()
    {
        return hasSetPid;
    }
    
    public boolean hasSetPauseGpsTime()
    {
        return hasSetPauseGpsTime;
    }

    public String toString()
    {
        StringBuffer sb = new StringBuffer();
        if (hasSetForeground)
            sb.append(", isForeground = "+isForeground);
        if (hasSetServerUrl)
            sb.append(", serverUrl = "+serverUrl);
        if (hasSetUserId)
            sb.append(", userId = "+userId);
        if (hasSetCarrierName)
            sb.append(", carrier = "+carrierName);
        if (hasSetRouteId)
            sb.append(", routeId = "+routeId);
        if (hasSetLogEnabled)
            sb.append(", logEnabled = "+logEnabled);
        if (hasSetAppVersion)
            sb.append(", appVersion = "+appVersion);
        if (hasSetAppName)
            sb.append(", appName = "+appName);
        if (hasSetForceStop)
            sb.append(", forceStop = "+isForceStop);
        if (hasSetPid)
            sb.append(", pid = "+pid);
        if (hasSetPauseGpsTime)
            sb.append(", pauseGpsTime = "+pauseGpsTime);
        
        return sb.toString();
    }
}
