package com.telenav.app.android.constant;

/**
 * Constants to indicate the common extra keys in intents.
 * 
 * @author wchshao (wchshao@telenav.cn)
 */
public final class IntentExtra
{
    private static final String PACKAGE = "com.telenav.app.android";
    
    /** share item */
    public static final String NATIVE_SHARE_ITEM = PACKAGE + ".shareitem";
    
    /** share subject */
    public static final String SUBJECT = PACKAGE + ".subject";

    /** share content */
    public static final String CONTENT = PACKAGE + ".content";

    /** share content */
    public static final String HTML_CONTENT = PACKAGE + ".htmlcontent";
    
    /** title of the share page */
    public static final String TITLE = PACKAGE + ".title";

    /** indicate whether it's the mixed mode */
    public static final String MIXED = PACKAGE + ".mixed";
}
