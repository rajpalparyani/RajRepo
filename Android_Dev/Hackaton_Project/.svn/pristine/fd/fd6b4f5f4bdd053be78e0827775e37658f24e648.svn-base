package com.telenav.log;

import com.telenav.logger.DefaultLoggerFilter;
import com.telenav.logger.Logger;

/**
 *@author fqming (fqming@telenav.cn)
 *@date Jul 23, 2010
 */
public class TnLoggerFilter extends DefaultLoggerFilter
{
    private static String enableClasses = "*";
    
    public TnLoggerFilter()
    {
        this(Logger.INFO, enableClasses);
    }
    
    public TnLoggerFilter(int level, String enableClasses)
    {
        super(level, enableClasses);
    }

}
